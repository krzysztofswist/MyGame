package com.krzysztof.m.swist.mygame;

public class Outbox {
	private int score;
	boolean over10points = false;
	boolean isEmpty() {
        return !over10points ;
    }
	public boolean isOver10points() {
		return over10points;
	}
	public void setOver10points(boolean over10points) {
		this.over10points = over10points;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
	
	
	
	

}
